/* miniprogram/pages/supermarket/me/me.wxss */
page {
background: white;
display: flex;
flex-direction: column;
justify-content: flex-start;
}

.userinfo {
margin-top: 100rpx;
display: flex;
flex-direction: column;
align-items: center;
}

.userinfo-avatar {
width: 150rpx;
height: 150rpx;
margin: 50rpx;
border-radius: 50%;
}

.userinfo-nickname {
  display: block;
  background-color: rgb(30, 199, 199);
  color: #FFF;
  border-radius: 3px;
  margin: 20px auto;
  padding: 10px;
}

.result_to_order {
    display:block;
    background-color:#69C3AA;
    color:#FFF;
    border-radius:3px;
    width:50%;
    text-align:center;
    margin:20px auto;
    padding:10px;
}